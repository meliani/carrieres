{
    "name": "laravel/laravel",
    "description": "INPT ENTREPRISES",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "amphp/parallel-functions": "^0.1.3",
        "barryvdh/laravel-dompdf": "^0.8.3",
        "barryvdh/laravel-snappy": "^0.4.3",
        "boolfalse/clearcaches": "^1.1",
        "doctrine/dbal": "~2.10",
        "facade/ignition": "^1.9",
        "fideloper/proxy": "~4.0",
        "geoip2/geoip2": "~2.0",
        "laravel-lang/lang": "~5.0",
        "laravel/framework": "^6.0",
        "laravel/helpers": "^1.4",
        "laravelcollective/errors": "^1.0",
        "laravelcollective/html": "^6.0",
        "livewire/livewire": "1.3.9",
        "maatwebsite/excel": "^3.1",
        "mockery/mockery": "~1.0",
        "pragmarx/tracker": "^4.0",
        "rap2hpoutre/laravel-log-viewer": "^1.3",
        "simplesoftwareio/simple-qrcode": "^2.0",
        "spatie/laravel-collection-macros": "^6.0",
        "spatie/laravel-flash": "^1.3",
        "spatie/laravel-medialibrary": "^7.0.0",
        "spatie/laravel-permission": "^5",
        "spatie/laravel-tags": "^2.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "~2.0",
        "laravel/telescope": "^2.0",
        "laravel/tinker": "~1.0",
        "laravel/ui": "^1.0",
        "nunomaduro/collision": "^3.0",
        "wemersonjanuario/wkhtmltopdf-windows": "0.12.2.3"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "minimum-stability": "dev",
        "prefer-stable": false
    }

}
