{
    "name": "laravel/laravel",
    "description": "INPT ENTREPRISES",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2.5|^8.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.6.10",

        "laravel/ui": "^3.0",
        "laravel/helpers": "^1.4",

        "laravel-lang/lang": "~5.0",

        "amphp/parallel-functions": "^0.1.3",
        "barryvdh/laravel-dompdf": "^1.0.0",
        "barryvdh/laravel-snappy": "^0.4.8",
        "boolfalse/clearcaches": "^1.1",
        "doctrine/dbal": "~2.6",
        "geoip2/geoip2": "~2.0",

        "laravelcollective/errors": "^1.0",
        "laravelcollective/html": "^6.0",

        "livewire/livewire": "^2.9.0",
        "maatwebsite/excel": "^3.1.x-dev",
        "rap2hpoutre/laravel-log-viewer": "^1.3",
        "simplesoftwareio/simple-qrcode": "^2.0",
        
        "spatie/laravel-collection-macros": "^7.10",
        "spatie/laravel-flash": "^1.9",
        "spatie/laravel-medialibrary": "^8.0",
        "spatie/laravel-permission": "^5.5",
        "spatie/laravel-tags": "^4.3.0"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0",

        "barryvdh/laravel-debugbar": "^3.6",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "~2.0",
        "laravel/telescope": "^4.x-dev",
        "laravel/tinker": "^2.5",
        "wemersonjanuario/wkhtmltopdf-windows": "0.12.2.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },

    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },

    "minimum-stability": "dev",
    "prefer-stable": true,

    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }

}
